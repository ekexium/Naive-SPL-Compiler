program system_functions;

function abs__(x:integer):integer;
begin
    if (x > 0) then abs__:=x
    else abs__:=0-x;
end;

function pred__(x:char):char;
begin
    pred__ := x - 1;
end;

function sqr__(x:integer):integer;
begin
    sqr__ := x * x;
end;

function succ__(x:char):char;
begin
    succ__ := x+1;
end;

