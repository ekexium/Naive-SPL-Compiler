program system_functions;


function abs(x:integer):integer;
begin
    if (x < 0) then begin abs := -x end
    else begin abs := x; end;
end;

function odd(x:integer):boolean;
begin
    if (x % 2 == 0) then begin odd := true end
    else begin odd := false; end;
end;

function pred(x:char):pred;
begin
    pred := x - 1;
end;

function sqr(x:integer):integer;
begin
    sqr := x * x;
end;

function sucsc(x:integer):integer;
begin
    sucsc:=x+1;
end
