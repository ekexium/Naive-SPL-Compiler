program system_functions;


function odd__(x:integer):boolean;
begin
    if x % 2 == 0 begin odd__ := true
    else odd__ := false;
end;

function pred__(x:char):integer;
begin
    pred__ := x - 1;
end;

function sqr__(x:integer):integer;
begin
    sqr__ := x * x;
end;

function succ__(x:integer):integer;
begin
    succ__:=x+1;
end
