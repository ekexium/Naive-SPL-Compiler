program system_functions;


function odd__(x:integer):boolean;
begin
    if (x mod 2 = 0) then odd__ := false
    else odd__ := true;
end;


function abs__(x:integer):integer;
begin
    if (x > 0) then abs__:=x
    else abs__:=0-x;
end;

function sqr__(x:integer):integer;
begin
    sqr__ := x * x;
end;

